# Creek Finance Bot - Environment Configuration Example
# Copy this file to .env and fill in your values

# SUI Network Configuration
SUI_NETWORK=testnet
SUI_RPC_URL=https://sui-testnet-rpc.publicnode.com

# Wallet Private Keys (Base64 encoded suiprivkey format)
# Option 1: Single wallet
SUI_PRIVATE_KEY=suiprivkey1qxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# Option 2: Multiple wallets (comma-separated)
# SUI_PRIVATE_KEYS=suiprivkey1qxxx...,suiprivkey1qyyy...,suiprivkey1qzzz...

# Proxy Configuration (optional)
# PROXY_1=http://user:pass@proxy1:port
# PROXY_2=http://user:pass@proxy2:port

# Bot Configuration
MIN_SUI_BALANCE=1.0
GAS_BUDGET=200000000

# Operation Counts
XAUM_CLAIM_COUNT=3
USDC_CLAIM_COUNT=3
STAKE_COUNT=3

# Safety Configuration
MAX_RETRIES=5
RATE_LIMIT_COOLDOWN=30

# Logging Level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO
